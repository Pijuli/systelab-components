    "license": "MIT",
    "keywords": [
        "Angular",
        "Angular-cli",
        "NPM",
        "Systelab"
    ],
    "private": false,
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "update-snapshots": "ng e2e --config protractor-update-snapshot.conf.js",
        "cleardown": "rimraf css sass html build widgets",
